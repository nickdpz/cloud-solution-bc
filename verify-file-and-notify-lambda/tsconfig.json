{
	"compilerOptions": {
		"declaration": false,
		"target": "es6",
		"module": "commonjs",
		"sourceMap": false,
		"moduleResolution": "node",
		"rootDir": ".",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"outDir": "dist/",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"types": ["jasmine"]
	},
	"include": ["src/**/*"],
	"exclude": ["dist/", "node_modules/**/*", "test/"]
}
